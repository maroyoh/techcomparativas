{
  "name": "techcomparativas-monorepo",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
"scripts": {
  "dev": "turbo run dev --filter=apps/web",
  "build": "turbo run build",
  "lint": "turbo run lint",
  "test": "turbo run test",
  "preview": "turbo run preview",
  "prisma:generate": "pnpm exec prisma generate --schema packages/db/schema.prisma",
  "postinstall": "pnpm exec prisma generate --schema packages/db/schema.prisma"
},
  "devDependencies": {
    "turbo": "^1.10.0",
    "typescript": "^5.3.3",
    "eslint": "^8.49.0",
    "prettier": "^3.1.0"
  },
  "packageManager": "pnpm@9.7.1"
}